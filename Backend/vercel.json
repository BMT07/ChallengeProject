{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/forum/(.*)",
            "dest": "/forum/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}